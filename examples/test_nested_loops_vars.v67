printf("Testing nested loops with variables\n")

@ i in 1..3 max 10 {
    @ j in 1..3 max 10 {
        x := i * 10 + j
        printf("i=%f, j=%f, x=%f\n", i, j, x)
        
        // Assertions
        x {
            (i * 10 + j) => { } // ok
            ~> {
                printf("ERROR: Mismatch at i=%f, j=%f! Got x=%f, expected %f\n", i, j, x, i * 10 + j)
                exit(1)
            }
        }
    }
}

printf("Nested loops test PASSED\n")

