import sdl3 as sdl

cstruct SDL_Event {
    type as uint32,
    timestamp as uint64
}

sdl.SDL_Init(sdl.SDL_INIT_VIDEO) or! {
    exitf("SDL_Init failed\n")
}
defer sdl.SDL_Quit()

println("About to malloc event")
event := c.malloc(192) as SDL_Event
println("Event malloced")

event {
    0 => {
        println("event is NULL!")
    }
    ~> {
        println("event is not NULL")
    }
}
